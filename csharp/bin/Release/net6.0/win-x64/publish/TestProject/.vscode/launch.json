{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug TestProject",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bins/TestProject.elf",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Program Files/Microchip/xc32/v4.60/bin/xc32-gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "makemake",
            "miDebuggerServerAddress": "localhost:3333",
            "debugServerPath": "openocd",
            "debugServerArgs": "-f interface/pickit4.cfg -f target/pic32mz.cfg",
            "serverStarted": "Info : Listening on port 3333 for gdb connections",
            "filterStderr": true,
            "customLaunchSetupCommands": [
                {
                    "text": "target remote localhost:3333"
                },
                {
                    "text": "monitor reset halt"
                },
                {
                    "text": "load"
                }
            ]
        }
    ]
}